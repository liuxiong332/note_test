
<%= link_to 'New', new_article_path %>
<table>
  <tr>
    <th>Title</th>
    <th>Text</th>
    <th>Edit</th>
    <th>Destroy</th>
  </tr>

  <% @articles.each do |article| %>
  <tr>
    <td><%= article.title %> </td>
    <td><%= article.text %> </td>
    <td><%= link_to 'Edit', edit_article_path(article) %> </td>
    <td><%= link_to 'Destroy', article_path(article), method: :delete %> </td>
  </tr>
  <% end %>
</table>
